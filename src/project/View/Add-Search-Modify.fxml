            fx:controller="project.Controller.ControllerAddSearch">
    <children>
        <Label fx:id="exit" onMousePressed="#handleClose" layoutX="25.0" prefHeight="36.0" prefWidth="225.0"
               text="Better IMDB" textFill="#e52019dc">
            <font>
                <Font size="25.0"/>
            </font>
        </Label>
        <AnchorPane layoutX="60.0" layoutY="287.0" prefHeight="90.0" prefWidth="190.0"
                    style="-fx-background-color: #1C1F25">
            <children>
                <Button fx:id="btn1" alignment="CENTER" layoutX="14.0" layoutY="12.0" mnemonicParsing="false"
                        onAction="#handleButtonAction" prefHeight="65.0" prefWidth="165.0"
                        style="-fx-focus-color: transparent" text="Add"/>
            </children>
        </AnchorPane>
        <AnchorPane layoutX="250.0" layoutY="287.0" prefHeight="90.0" prefWidth="190.0"
                    style="-fx-background-color: #1C1F25">
            <children>
                <Button fx:id="btn2" alignment="CENTER" layoutX="14.0" layoutY="12.0" mnemonicParsing="false"
                        onAction="#handleButtonAction" prefHeight="65.0" prefWidth="160.0" text="Search"/>
            </children>
        </AnchorPane>
        <AnchorPane layoutX="431.0" layoutY="287.0" prefHeight="90.0" prefWidth="190.0"
                    style="-fx-background-color: #1C1F25">
            <children>
                <Button fx:id="btn21" alignment="CENTER" layoutX="14.0" layoutY="12.0" mnemonicParsing="false"
                        onAction="#handleButtonModify" prefHeight="65.0" prefWidth="160.0" text="Modify"/>
            </children>
        </AnchorPane>
    </children>
    <opaqueInsets>
    </opaqueInsets>
</AnchorPane>
